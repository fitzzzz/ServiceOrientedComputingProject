<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IVelibService" maxReceivedMessageSize="30000000"/>
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/VelibSoapService/VelibService/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IVelibService"
                contract="VelibService.IVelibService" name="WSDualHttpBinding_IVelibService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>